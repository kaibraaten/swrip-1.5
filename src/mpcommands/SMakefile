LIBRARIAN = oml
RM = delete quiet force
OBJ = mpwithdraw.o mptransfer.o mpslay.o mprestore.o mppurge.o mpopenpassage.o mpoload.o mpnothing.o mpmload.o mpkill.o mpjunk.o mpinvis.o mpgoto.o mpgain.o mpforce.o mpechoat.o mpechoaround.o mpecho.o mpdream.o mpdeposit.o mpdamage.o mpclosepassage.o mpat.o mpasound.o mpapplyb.o mpapply.o

LIBNAME = mpcommands.lib

all:		$(LIBNAME)

.cpp.o:
		$(CC) $*.cpp

.c.o:
		$(CC) $*.c

$(LIBNAME):	$(OBJ)
		$(LIBRARIAN) $@ r $(OBJ)

clean:
		$(RM) \#?.o $(LIBNAME) \#?~
