LIBRARIAN = oml
RM = delete quiet force
OBJ = bet.o event.o file_io.o linkedlist.o misc.o os_amiga.o repository.o repositorybase.o sha256.o string_handling.o time_fun.o vector3.o
LIBNAME = utility.lib

all:		$(LIBNAME)

.cpp.o:
		$(CC) $*.cpp

.c.o:
		$(CC) $*.c

$(LIBNAME):	$(OBJ)
		$(LIBRARIAN) $@ r $(OBJ)

clean:
		$(RM) \#?.o $(LIBNAME) \#?~
